<tal:sections tal:define="portal_tabs view/portal_tabs"
     tal:condition="portal_tabs"
     i18n:domain="plone">
    <p class="hiddenStructure" i18n:translate="heading_sections">Navigation</p>

    <ul id="portal-globalnav"
        tal:define="selected_tab python:view.selected_portal_tab"
        ><tal:tabs tal:repeat="tab portal_tabs"
        ><li tal:define="tid tab/id;name tab/name"
             tal:condition="python:name!='Home'"
            ><div tal:attributes="id string:portaltab-${tid};"><a href=""
               tal:content="tab/name"
               tal:attributes="href tab/url;
                               title tab/description|nothing;">
            Tab Name
            </a></div></li></tal:tabs></ul>


        <tal:tabs tal:repeat="tab portal_tabs">
            <div tal:define="tid tab/id;name tab/name"
                tal:attributes="id string:portalsubtabs-${tid};class string:portalsubtabs"
                tal:condition="python:tid in ('topics', 'policy', 'data', 'countries_and_networks', 'networks')">
                <tal:block omit-tag="" tal:condition="python:tid == 'topics'">
                    <div class="column">
                        <ul class="first-level">
                            <li class="list-title">Status</li>
                            <ul class="second-level">
                                <li><a tal:define="url tab/url" tal:attributes="href python:'/'.join({url + '/ecosystems-and-habitats'})">Ecosystems and habitats</a></li>
                                <ul class="third-level">
                                  <li><a tal:define="url tab/url" tal:attributes="href python:'/'.join({url + '/ecosystems-and-habitats/agro-ecosystems-and-grasslands'})"> - Agro-ecosystems and grasslands</a></li>
                                  <li><a tal:define="url tab/url" tal:attributes="href python:'/'.join({url + '/ecosystems-and-habitats/coastal'})"> - Coastal</a></li>
                                  <li><a tal:define="url tab/url" tal:attributes="href python:'/'.join({url + '/ecosystems-and-habitats/forest'})"> - Forest</a></li>
                                  <li><a tal:define="url tab/url" tal:attributes="href python:'/'.join({url + '/ecosystems-and-habitats/heath-and-schrubs'})"> - Heath and Scrubs</a></li>
                                  <li><a tal:define="url tab/url" tal:attributes="href python:'/'.join({url + '/ecosystems-and-habitats/ice-rock-polar'})"> - Ice, rock, polar</a></li>
                                  <li><a tal:define="url tab/url" tal:attributes="href python:'/'.join({url + '/ecosystems-and-habitats/islands'})"> - Islands</a></li>
                                  <li><a tal:define="url tab/url" tal:attributes="href python:'/'.join({url + '/ecosystems-and-habitats/lakes-rivers-and-wetlands'})"> - Lakes, rivers and wetlands</a></li>
                                  <li><a tal:define="url tab/url" tal:attributes="href python:'/'.join({url + '/ecosystems-and-habitats/marine'})"> - Marine</a></li>
                                  <li><a tal:define="url tab/url" tal:attributes="href python:'/'.join({url + '/ecosystems-and-habitats/mountains'})"> - Mountains</a></li>
                                  <li><a tal:define="url tab/url" tal:attributes="href python:'/'.join({url + '/ecosystems-and-habitats/urban'})"> - Urban</a></li>             
                                </ul>
                                <li><a tal:define="url tab/url" tal:attributes="href python:'/'.join({url + '/species'})">Species</a></li>
                                <ul class="third-level">
                                  <li><a tal:define="url tab/url" tal:attributes="href python:'/'.join({url + '/species/amphibians'})"> - Amphibians</a></li> 
                                  <li><a tal:define="url tab/url" tal:attributes="href python:'/'.join({url + '/species/mammals'})"> - Mammals</a></li> 
                                  <li><a tal:define="url tab/url" tal:attributes="href python:'/'.join({url + '/species/reptiles'})"> - Reptiles</a></li> 
                                </ul>
                                <li><a tal:define="url tab/url" tal:attributes="href python:'/'.join({url + '/genetic-resources'})">Genetic resources</a></li>
                            </ul>
                        </ul>
                    </div>
                    <div class="column">
                        <ul class="first-level">
                            <li class="list-title">Threats</li>
                            <ul class="second-level">
                              <li><a tal:define="url tab/url" tal:attributes="href python:'/'.join({url + '/climate-change'})">Climate change</a></li>
                              <li><a tal:define="url tab/url" tal:attributes="href python:'/'.join({url + '/invasive-species'})">Invasive species</a></li>
                              <li><a tal:define="url tab/url" tal:attributes="href python:'/'.join({url + '/fragmentation'})">Fragmentation</a></li>
                              <li><a tal:define="url tab/url" tal:attributes="href python:'/'.join({url + '/land-use-change'})">Land use change</a></li>
                              <li><a tal:define="url tab/url" tal:attributes="href python:'/'.join({url + '/pollution'})">Pollution</a></li>
                              <li><a tal:define="url tab/url" tal:attributes="href python:'/'.join({url + '/overexploitation'})">Overexploitation</a></li>                              
                            </ul>
                            <li class="list-title">Impacts</li>
                            <ul class="second-level">
                              <li><a tal:define="url tab/url" tal:attributes="href python:'/'.join({url + '/tipping-points'})">Tipping points</a></li>     
                            </ul>  
                            <li class="list-title">Benefits</li>
                            <ul class="second-level">
                              <li><a tal:define="url tab/url" tal:attributes="href python:'/'.join({url + '/ecosystem-services'})">Ecosystem services</a></li>     
                            </ul> 
                            <li class="list-title">Responses</li>
                            <ul class="second-level">
                              <li><a tal:define="url tab/url" tal:attributes="href python:'/'.join({url + '/protected-areas'})">Protected areas</a></li>    
                              <li><a tal:define="url tab/url" tal:attributes="href python:'/'.join({url + '/green-infraestructure'})">Green infraestructure</a></li>   
                              <li><a tal:define="url tab/url" tal:attributes="href python:'/'.join({url + '/life-nature-and-biodiversity-projects'})">LIFE+ Nature and Biodiversity Projects</a></li>                                                                
                            </ul> 
                        </ul>
                    </div>                    
                    <div class="column">
                      <ul class="first-level">
                        <li class="list-title">Flagship projects</li>
                        <ul class="second-level">
                          <li><a href="http://ec.europa.eu/environment/nature/biodiversity/comm2006/index_en.htm">BAP report</a> 
                            <span class="description">Biodiversity Action Plan Report</span></li>
                          <li><a href="http://www.eea.europa.eu/publications/eu-2010-biodiversity-baseline/">EU 2010 Biodiversity Baseline report</a> 
                            <span class="description">Biodiversity Baseline report</span></li>
                          <li><a  tal:define="url tab/url" tal:attributes="href python:'/'.join({url + '/sebi-indicators'})">SEBI 2010 indicators</a>
                            <span class="description">Streamlining European Biodiversity Indicators</span></li>
                          <li><a href="http://www.eea.europa.eu/soer">SOER 2010</a>
                            <span class="description">State of the Environment Report</span></li>
                          <li><a href="http://www.teebweb.org/">TEEB study</a> 
                            <span class="description">The Economics of Ecosystems and Biodiversity</span></li>
                          <li><a href="http://www.eea.europa.eu/publications/10-messages-for-2010">10 Messages for Biodiversity</a></li>
                        </ul>
                      </ul>
                    </div>                    
                </tal:block>
                <tal:block omit-tag="" tal:condition="python:tid == 'policy'">
                    <div class="column">
                      <ul class="first-level">
                        <li class="list-title">Biodiversity strategy 2020</li>
                        <ul class="second-level">
                          <li><a href="http://ec.europa.eu/environment/nature/biodiversity/comm2006/2020.htm">EU strategy to 2020</a></li>

                          <li><a tal:define="url tab/url" tal:attributes="href python:'/'.join({url + '/eu-biodiversity-strategy'})">EU Targets and Actions overview</a></li>
                          <ul class="third-level">
                            <li><a href="/policy/eu-biodiversity-strategy/target-1-and-related-aichi-targets">- EU Targets mapped to Aichi Targets</a></li>
                            <li><a href="http://www.cbd.int/sp/targets/">- Aichi Targets to 2020</a></li>
                          </ul>
                        </ul>
                      </ul>
                </div>                    
                </tal:block>
                <tal:block omit-tag="" tal:condition="python:tid == 'data'">
                    <div class="column">
                        <ul class="first-level">
                            <li class="list-title">Biodiversity data centre (BDC)</li>
                            <ul class="second-level">
                                <li>
                                    <a href="http://www.eea.europa.eu/themes/biodiversity/dc">BDC overview</a>
                                </li>
                                <ul class="third-level">
                                    <li>
                                        <a href="http://natura2000.eea.europa.eu/">- Natura 2000 map viewer</a>
                                    </li>
                                    <li>
                                        <a href="http://www.eea.europa.eu/data-and-maps/52E54BF3-ACDB-4959-9165-F3E4469BE610">- Natura 2000 data</a>
                                    </li>
                                </ul>
                                <li>
                                    <a href="http://eunis.eea.europa.eu/">EUNIS</a>
                                </li>
                                <li>
                                    <a href="http://www.eea.europa.eu/themes/biodiversity/indicators">Indicators</a>
                                </li>
 
                            </ul>
                        </ul>
                    </div>
                    <div class="column">
                      <ul class="first-level">
                        <li class="list-title">Other enviromental data centres from EU</li>  
                        <ul class="second-level">
                          <li><a href="http://www.eea.europa.eu/themes/air/dc">Air</a></li>
                          <li><a href="http://www.eea.europa.eu/themes/climate/dc">Climate change</a></li>
                          <li><a href="http://efdac.jrc.ec.europa.eu/">Forest</a></li>
                          <li><a href="http://www.eea.europa.eu/themes/landuse/dc">Land use</a></li>
                          <li><a href="http://epp.eurostat.ec.europa.eu/statistics_explained/index.php/Environmental_Data_Centre_on_Natural_Resources_and_Products">Natural resources and products</a></li>
                          <li><a href="http://eusoils.jrc.ec.europa.eu/data.html">Soil</a></li>
                          <li><a href="http://epp.eurostat.ec.europa.eu/portal/page/portal/waste/introduction">Waste</a></li>
                          <li><a href="http://www.eea.europa.eu/themes/water/dc">Water</a></li>
                        </ul>
                      </ul>
                    </div>
                </tal:block>
                <tal:block omit-tag="" tal:condition="python:tid == 'countries_and_networks'">
                    <div class="column">
                        <ul class="first-level">
                            <li class="list-title">EU Biodiversity action plan (BAP)</li>
                            <ul class="second-level">

                                <li><a tal:define="url tab/url" tal:attributes="href python:'/'.join({url + '/bap/community_report/'})">BAP 2010 Community report</a></li>
                                <li><a tal:define="url tab/url" tal:attributes="href python:'/'.join({url + '/bap/compare/'})">BAP 2010 comparative view</a></li>
                            </ul>
                        </ul>
                    </div>                   
 
                </tal:block>
                <tal:block omit-tag="" tal:condition="python:tid == 'networks'">
                    <div class="column">
                        <ul class="first-level">
                            <li class="list-title">European Network of Biodiversity CHMs</li>
                            <ul class="second-level">
                                <li><a href="/chm-network/">Clearing House Mechanism (CHM) network</a></li>
                            </ul>
                            <li class="list-title">EIONET</li>
                            <ul class="second-level">
                                <li><a href="http://forum.eionet.europa.eu/nrc-nature-and-biodiversity-interest-group/">Forum - NRC Nature and biodiversity</a></li>
                                <li><a href="http://bd.eionet.europa.eu/">European Topic Centre on Biological Diversity</a></li>
                            </ul>
                        </ul>
                    </div>
                </tal:block>
             </div>
            
        </tal:tabs>            
</tal:sections>
